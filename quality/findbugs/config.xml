<!--
FindBugs exclude filter
Severity: HARD
-->
<FindBugsFilter>
    <Match>
        <Class name="~.*\.BuildConfig"/>
    </Match>
    <Match>
        <Class name="~.*\.R"/>
    </Match>
    <Match>
        <Class name="~.*\.R\$.*"/>
    </Match>

    <Match>
        <Bug category="SF_SWITCH_NO_DEFAULT"/>
    </Match>
</FindBugsFilter>
